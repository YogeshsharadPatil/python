     "text": [
      "Enter a Number :->5\n",
      " 5 not a prime number\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter a Number :->\"))\n",
    "for i in range(2,n):\n",
    "    if (n>0 & n%i==0):\n",
    "        print(\" {} not a prime number\".format(n))\n",
    "        break\n",
    "else :\n",
    "    print(\" {}  a prime number\".format(n))\n",
    "\n",

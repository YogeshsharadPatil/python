    "n = int(input(\"Enter a number: \"))\n",
    "print(\"Sum of first {} natural number is :\".format(n))\n",
    "\n",
    "def sumi_of_squares(n):\n",
    "    sum = 0\n",
    "    for i in range(n+1):\n",
    "        i = i**2\n",
    "        sum = sum + i\n",
    "        i+=1\n",
    "    \n",
    "    \n",
    "    return sum\n",
    "        \n",
